{
  "extends": "../tsconfig.json",
  "jsx": "react-jsx",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "outDir": "build",
    "rootDir": "src",
    "allowJs": true,
    "isolatedModules": true,
    "noEmit": true,
    "target": "es5"
  },
  "include": ["src", ".eslintrc.js"],
}
